{
  "name": "typesafe-templates",
  "version": "1.2.10",
  "description": "Template engine for writing compiler-checked templates in TypeScript by leveraging JSX to generate JavaScript code from TypeScript code files rather than text templates.",
  "author": {
    "name": "Matt Strom",
    "email": "support@mattstrom.com"
  },
  "license": "ISC",
  "main": "dist/app/index.js",
  "typings": "dist/app/index.d.ts",
  "scripts": {
    "clean": "rimraf dist dist-tests",
    "prebuild": "npm run clean",
    "prebuild:tests": "npm run prebuild",
    "build": "tsc --build src/app",
    "build:tests": "tsc --build src/tests",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build",
    "commit": "git-cz",
    "release": "semantic-release --no-ci",
    "release:dry-run": "semantic-release --no-ci --dry-run",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm test"
    }
  },
  "keywords": [
    "typescript",
    "tsx",
    "templates",
    "template engine",
    "javascript",
    "jsx",
    "tooling"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mattstrom/typesafe-templates"
  },
  "files": [
    "dist/app"
  ],
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/generator": "^7.4.4",
    "@babel/parser": "^7.1.3",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/traverse": "^7.1.4",
    "@babel/types": "^7.1.3",
    "babel-literal-to-ast": "^2.1.0",
    "outdent": "^0.7.0",
    "snyk": "^1.279.1"
  },
  "peerDependencies": {
    "ejs": "^2.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/commit-analyzer": "^7.0.0",
    "@semantic-release/git": "^8.0.0",
    "@semantic-release/github": "^6.0.1",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "@types/babel-types": "^7.0.4",
    "@types/babel__core": "^7.0.1",
    "@types/babel__generator": "^7.0.2",
    "@types/babel__parser": "^7.0.0",
    "@types/babel__traverse": "^7.0.0",
    "@types/ejs": "^3.0.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^10.17.13",
    "@types/prettier": "^1.16.4",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "ejs": "^3.0.1",
    "husky": "^4.2.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "rimraf": "^3.0.0",
    "semantic-release": "^16.0.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.11.0",
    "typescript": "^3.4.5"
  },
  "snyk": true
}
